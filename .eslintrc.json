{
  "plugins": ["simple-import-sort"],
  "extends": [
    "eslint:recommended",
    "next",
    "next/core-web-vitals",
    "prettier"
  ],
  "root": true,
  "rules": {
    "no-console": "warn",
    "no-unused-vars": "warn",
    "no-undef": "error",
    "simple-import-sort/imports": "error",
    "simple-import-sort/exports": "error"
  }
}